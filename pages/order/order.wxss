page{
    min-height: 100%;
    background: #f4f4f4;
}

.nf-user{
    padding: 10rpx 0;
    background: #ffffff;
}
.nf-user-add-adress{
    padding: 5rpx 15rpx;
}
.nf-add-address-btn{
    color: #14baff;
    background: #ffffff;
    border: 1rpx solid #14baff;
    font-size: 14px;
}
.nf-address-line{
    height: 4rpx;
    margin-bottom: 20rpx;
    background: url(data:image/png;base64,) repeat-x;
}
.nf-user-address{
    position: relative;
    display: flex;
    height: 120rpx;
    flex-direction: column;
    padding: 0 25rpx;
}
.nf-user-address-edit{
    position: absolute;
    top: 28rpx;
    right: 20rpx;
    width: 40rpx;
    height: 40rpx;
}
.nf-user-info{
    flex: 1;
    height: 30rpx;
    display: flex;
    font-size: 14px;
}
.nf-user-address-info{
    flex: 1;
    padding-top: 10rpx;
    font-size: 14px;
    vertical-align: middle;
}
.nf-user-address-info text{
    font-size: 14px;
}
.nf-user-name{
    flex: 1;
    line-height: 60rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-weight: bold
}
.nf-user-phone{
    flex: 1;
    line-height: 60rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-weight: bold
}
.nf-user-default{
    padding: 5rpx 15rpx;
    margin: 0 15rpx;
    background: #fc1318;
    color: #ffffff;
    border-radius: 2px;
    font-size: 12px;
}
.nf-address-icon{
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}
.nf-shop-warp{
    margin: 20rpx 0;
    padding: 0 20rpx;
    background: #ffffff;
}
.nf-shop-home{
    border-bottom: 1rpx solid #ebebeb;
    color: #14baff;
    font-size: 12px;
    line-height: 40px;
}
.nf-shop-home image{
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}
.nf-shop-info{
    display: flex;
    padding: 15rpx;
    background: #ffffff;
    border-bottom: 1rpx solid #ebebeb;
}
.nf-shop-info:last-child{
    border-bottom: 0;
}
.nf-shop-info-img{
    width: 135rpx;
    height: 135rpx;
}
.nf-shop-text{
    flex: 1;
    display: flex;
    padding-left: 15rpx;
    overflow: hidden;
    flex-direction: column;
    justify-content: space-between;
}
.nf-shop-title{
    flex: 1;
    padding: 10rpx 0;
    font-size: 14px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.nf-shop-price{
    flex: 1;
    padding: 5rpx 0;
    display: flex;
}
.nf-shop-price-item{
    flex: 1;
    font-size: 12px;
}
.nf-shop-foot{
    flex: 1;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.nf-shop-price-name{
    color: #999999;
}
.nf-shop-price-num{
    color: #fc1318;
    padding: 0 10rpx;
}
.nf-shop-price-unit{
    color: #333333;
}
.nf-shop-all-price{
    color: #fc1318;
    padding: 0 10rpx;
    font-size: 14px;
    font-weight: bold;
}
.nf-shop-pay{
    font-size: 12px;
    text-align: right;
}
.nf-shop-other-list{
    padding: 0 20rpx;
    background: #ffffff;
}
.nf-shop-list-item{
    position: relative;
    display: flex;
    height: 80rpx;
    border-bottom: 1px solid #ebebeb;
}
.nf-item-text{
    flex: 1;
    line-height: 80rpx;
    font-size: 14px;
}
.nf-shop-list-item input,.nf-shop-express-price{
    flex: 2;
    line-height: 80rpx;
    font-size: 14px;
}
.nf-express-way{
    line-height: 80rpx;
    font-size: 14px;
}
.nf-shop-icon{
    position: absolute;
    right: 20rpx;
    top: 26rpx;
    width: 40rpx;
    height: 40rpx;
}

.nf-shop-list-item input{
    height: auto;
    width: auto;
    margin: 10rpx 0;
}
.nf-shop-express-price text{
    color: #fc1318;
}
.nf-price-compute{
    margin-top: 20rpx;
    padding: 20rpx 0;
    background: #ffffff;
}
.nf-price-notice{
    text-align: center;
    font-size: 18px;
    line-height: 60rpx;
}
.nf-price-notice text{
    font-size: 26px;
    color: #fc1318;
    font-weight: bold;
}
.nf-price-notice-express{
    text-align: center;
    font-size: 14px;
}
.nf-price-pay-money{
    padding: 20rpx;
}
.nf-shop-notice{
    padding: 10rpx 0;
    color: #ff2d2d;
    font-size: 12px;
}
.nf-shop-notice image{
    width: 20rpx;
    height: 20rpx;
    vertical-align: middle;
}